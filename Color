--== GUI SETUP ==--
local player = game.Players.LocalPlayer
local screenGui = Instance.new("ScreenGui")
screenGui.Name = "TheInception"
screenGui.ResetOnSpawn = false
screenGui.Parent = player:WaitForChild("PlayerGui")

-- Main frame
local mainFrame = Instance.new("Frame")
mainFrame.Size = UDim2.new(0,350,0,400)
mainFrame.Position = UDim2.new(0,50,0,50)
mainFrame.BackgroundColor3 = Color3.fromRGB(40,40,40)
mainFrame.BorderSizePixel = 0
mainFrame.Parent = screenGui

-- Title
local titleLabel = Instance.new("TextLabel")
titleLabel.Size = UDim2.new(1,0,0,50)
titleLabel.Text = "üé® The Inception"
titleLabel.Font = Enum.Font.Fantasy
titleLabel.TextSize = 24
titleLabel.TextColor3 = Color3.new(1,1,1)
titleLabel.BackgroundColor3 = Color3.fromRGB(20,20,20)
titleLabel.Parent = mainFrame

-- Close
local closeButton = Instance.new("TextButton")
closeButton.Size = UDim2.new(0,80,0,30)
closeButton.Position = UDim2.new(1,-85,0,10)
closeButton.Text = "‚ùå Close"
closeButton.Font = Enum.Font.GothamBold
closeButton.TextSize = 18
closeButton.BackgroundColor3 = Color3.fromRGB(180,50,50)
closeButton.TextColor3 = Color3.new(1,1,1)
closeButton.Parent = mainFrame
closeButton.MouseButton1Click:Connect(function()
	mainFrame.Visible = false
end)

-- 
local scrollFrame = Instance.new("ScrollingFrame")
scrollFrame.Size = UDim2.new(1,-10,1,-150)
scrollFrame.Position = UDim2.new(0,5,0,55)
scrollFrame.ScrollBarThickness = 10
scrollFrame.BackgroundTransparency = 1
scrollFrame.Parent = mainFrame

local UIListLayout = Instance.new("UIListLayout")
UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
UIListLayout.Padding = UDim.new(0,5)
UIListLayout.Parent = scrollFrame

local function updateCanvasSize()
	scrollFrame.CanvasSize = UDim2.new(0,0,0,UIListLayout.AbsoluteContentSize.Y + 10)
end
UIListLayout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(updateCanvasSize)
updateCanvasSize()

--==  ==--
local UserInputService = game:GetService("UserInputService")

local dragging = false
local dragInput
local dragStart
local startPos

local function update(input)
	local delta = input.Position - dragStart
	mainFrame.Position = UDim2.new(
		startPos.X.Scale,
		startPos.X.Offset + delta.X,
		startPos.Y.Scale,
		startPos.Y.Offset + delta.Y
	)
end

titleLabel.InputBegan:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 then
		dragging = true
		dragStart = input.Position
		startPos = mainFrame.Position

		input.Changed:Connect(function()
			if input.UserInputState == Enum.UserInputState.End then
				dragging = false
			end
		end)
	end
end)

titleLabel.InputChanged:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseMovement then
		dragInput = input
	end
end)

UserInputService.InputChanged:Connect(function(input)
	if input == dragInput and dragging then
		update(input)
	end
end)


--== Rainbow colors ==--
local rainbowColors = {
	Color3.fromRGB(255,90,90),
	Color3.fromRGB(255,170,90),
	Color3.fromRGB(255,230,90),
	Color3.fromRGB(120,220,120),
	Color3.fromRGB(100,200,255),
	Color3.fromRGB(140,140,255),
	Color3.fromRGB(190,140,255),
	Color3.fromRGB(255,140,200),
}
local rainbowIndex = 1

--== Helper tables ==
local selectedItems = {}
local checkboxMap = {}
local categoryItems = {}
local subCategoryItems = {}

--== SubCategory ==
local function createSubCategory(parent, catName, name, items)
	local subCatFrame = Instance.new("Frame")
	subCatFrame.Size = UDim2.new(1,0,0,30)
	subCatFrame.BackgroundColor3 = Color3.fromRGB(70,70,70)
	subCatFrame.BorderSizePixel = 0
	subCatFrame.Parent = parent

	local subLabel = Instance.new("TextButton")
	subLabel.Size = UDim2.new(1,0,1,0)
	subLabel.Text = "‚ñ∂ "..name
	subLabel.Font = Enum.Font.GothamBold
	subLabel.TextSize = 18
	subLabel.TextColor3 = Color3.new(1,1,1)
	subLabel.BackgroundTransparency = 1
	subLabel.Parent = subCatFrame

	local itemsVisible = false
	local itemFrames = {}
	subCategoryItems[name] = {}

	subLabel.MouseButton1Click:Connect(function()
		itemsVisible = not itemsVisible
		subLabel.Text = (itemsVisible and "‚ñº " or "‚ñ∂ ")..name
		for _, f in ipairs(itemFrames) do
			f.Visible = itemsVisible
		end
		updateCanvasSize()
	end)

	local subSelected = false
	subLabel.MouseButton2Click:Connect(function()
		subSelected = not subSelected
		subLabel.TextColor3 = subSelected and Color3.fromRGB(50,255,50) or Color3.new(1,1,1)
		for _, itemName in ipairs(items) do
			selectedItems[itemName] = subSelected
			if checkboxMap[itemName] then
				checkboxMap[itemName].Text = subSelected and "‚òë" or "‚òê"
			end
		end
	end)

	for _, itemName in ipairs(items) do
		local itemFrame = Instance.new("Frame")
		itemFrame.Size = UDim2.new(1,0,0,25)
		itemFrame.BackgroundColor3 = Color3.fromRGB(80,80,80)
		itemFrame.BorderSizePixel = 0
		itemFrame.Visible = false
		itemFrame.Parent = parent

		local checkBox = Instance.new("TextButton")
		checkBox.Size = UDim2.new(0,25,1,0)
		checkBox.Position = UDim2.new(0,5,0,0)
		checkBox.Text = "‚òê"
		checkBox.Font = Enum.Font.GothamBold
		checkBox.TextSize = 18
		checkBox.BackgroundColor3 = Color3.fromRGB(100,100,100)
		checkBox.TextColor3 = Color3.new(1,1,1)
		checkBox.Parent = itemFrame

		local itemLabel = Instance.new("TextLabel")
		itemLabel.Size = UDim2.new(1,-35,1,0)
		itemLabel.Position = UDim2.new(0,35,0,0)
		itemLabel.Text = itemName
		itemLabel.Font = Enum.Font.Gotham
		itemLabel.TextSize = 18
		itemLabel.TextColor3 = Color3.new(1,1,1)
		itemLabel.BackgroundTransparency = 1
		itemLabel.Parent = itemFrame

		local selected = false
		checkBox.MouseButton1Click:Connect(function()
			selected = not selected
			checkBox.Text = selected and "‚òë" or "‚òê"
			selectedItems[itemName] = selected
		end)

		checkboxMap[itemName] = checkBox
		table.insert(itemFrames, itemFrame)
		table.insert(subCategoryItems[name], itemName)
		table.insert(categoryItems[catName], itemName)
	end
end

--== Big Category (RIGHT CLICK SELECT) ==--
local function createCategory(name)
	local bgColor = rainbowColors[rainbowIndex]
	rainbowIndex += 1
	if rainbowIndex > #rainbowColors then
		rainbowIndex = 1
	end

	local container = Instance.new("Frame")
	container.Size = UDim2.new(1,0,0,35)
	container.BackgroundTransparency = 1
	container.Parent = scrollFrame

	local containerLayout = Instance.new("UIListLayout")
	containerLayout.SortOrder = Enum.SortOrder.LayoutOrder
	containerLayout.Padding = UDim.new(0,2)
	containerLayout.Parent = container

	containerLayout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
		container.Size = UDim2.new(1,0,0,containerLayout.AbsoluteContentSize.Y)
		updateCanvasSize()
	end)

	-- Header
	local header = Instance.new("Frame")
	header.Size = UDim2.new(1,0,0,35)
	header.BackgroundColor3 = bgColor:Lerp(Color3.fromRGB(25,25,25), 0.35)
	header.BorderSizePixel = 0
	header.Parent = container

	local corner = Instance.new("UICorner")
	corner.CornerRadius = UDim.new(0,8)
	corner.Parent = header

	local label = Instance.new("TextButton")
	label.Size = UDim2.new(1,0,1,0)
	label.Text = "‚ñ∂ "..name
	label.Font = Enum.Font.GothamBold
	label.TextSize = 20
	label.TextColor3 = Color3.new(1,1,1)
	label.BackgroundTransparency = 1
	label.Parent = header

	-- Sub container
	local subContainer = Instance.new("Frame")
	subContainer.Size = UDim2.new(1,0,0,0)
	subContainer.BackgroundTransparency = 1
	subContainer.Visible = false
	subContainer.Parent = container

	local subLayout = Instance.new("UIListLayout")
	subLayout.SortOrder = Enum.SortOrder.LayoutOrder
	subLayout.Padding = UDim.new(0,2)
	subLayout.Parent = subContainer

	subLayout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
		subContainer.Size = UDim2.new(1,0,0,subLayout.AbsoluteContentSize.Y)
	end)

	local expanded = false
	label.MouseButton1Click:Connect(function()
		expanded = not expanded
		subContainer.Visible = expanded
		label.Text = (expanded and "‚ñº " or "‚ñ∂ ")..name
	end)

	-- RIGHT CLICK = Select / Deselect all inside
	local selected = false
	label.MouseButton2Click:Connect(function()
		selected = not selected
		label.TextColor3 = selected and Color3.fromRGB(50,255,50) or Color3.new(1,1,1)

		for _, itemName in ipairs(categoryItems[name]) do
			selectedItems[itemName] = selected
			if checkboxMap[itemName] then
				checkboxMap[itemName].Text = selected and "‚òë" or "‚òê"
			end
		end
	end)

	categoryItems[name] = {}

	return subContainer
end


--== RGB Color Picker ==
local rBox = Instance.new("TextBox")
rBox.Size = UDim2.new(0,50,0,30)
rBox.Position = UDim2.new(0,0,1,-55)
rBox.Text = "255"
rBox.ClearTextOnFocus = false
rBox.Parent = mainFrame

local gBox = Instance.new("TextBox")
gBox.Size = UDim2.new(0,50,0,30)
gBox.Position = UDim2.new(0,60,1,-55)
gBox.Text = "0"
gBox.ClearTextOnFocus = false
gBox.Parent = mainFrame

local bBox = Instance.new("TextBox")
bBox.Size = UDim2.new(0,50,0,30)
bBox.Position = UDim2.new(0,120,1,-55)
bBox.Text = "0"
bBox.ClearTextOnFocus = false
bBox.Parent = mainFrame

local preview = Instance.new("Frame")
preview.Size = UDim2.new(0,40,0,30)
preview.Position = UDim2.new(0,180,1,-55)
preview.BackgroundColor3 = Color3.fromRGB(255,0,0)
preview.Parent = mainFrame

local function clamp(v)
	return math.clamp(tonumber(v) or 0,0,255)
end

local function getColor()
	return Color3.fromRGB(clamp(rBox.Text),clamp(gBox.Text),clamp(bBox.Text))
end

for _, box in ipairs({rBox,gBox,bBox}) do
	box:GetPropertyChangedSignal("Text"):Connect(function()
		preview.BackgroundColor3 = getColor()
	end)
end

local colorPicker = Instance.new("TextButton")
colorPicker.Size = UDim2.new(0,110,0,30)
colorPicker.Position = UDim2.new(0,230,1,-55)
colorPicker.Text = "üé® Apply Color"
colorPicker.Parent = mainFrame

colorPicker.MouseButton1Click:Connect(function()
	local editorRig = workspace:FindFirstChild("EditorRig")
	if not editorRig then return end
	local color = getColor()
	for _, part in ipairs(editorRig:GetDescendants()) do
		if part:IsA("BasePart") and selectedItems[part.Name] then
			part.Color = color
		end
	end
end)

--== Deselect All ==
local deselectButton = Instance.new("TextButton")
deselectButton.Size = UDim2.new(0,150,0,30)
deselectButton.Position = UDim2.new(0,0,1,-90)
deselectButton.Text = "‚ùå Deselect All"
deselectButton.Font = Enum.Font.GothamBold
deselectButton.TextSize = 18
deselectButton.BackgroundColor3 = Color3.fromRGB(180,50,50)
deselectButton.TextColor3 = Color3.new(1,1,1)
deselectButton.Parent = mainFrame

deselectButton.MouseButton1Click:Connect(function()
	for itemName,_ in pairs(selectedItems) do
		selectedItems[itemName] = false
		if checkboxMap[itemName] then
			checkboxMap[itemName].Text = "‚òê"
		end
	end
end)

