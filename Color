for bigCatName, subCats in pairs(categories) do
	local subContainer = createCategory(bigCatName)
	for subName, items in pairs(subCats) do
		createSubCategory(subContainer, bigCatName, subName, items)
	end
end


--== Favorite Colors Button ==
local favoriteButton = Instance.new("TextButton")
favoriteButton.Size = UDim2.new(0,120,0,30)
favoriteButton.Position = UDim2.new(0,230,1,-90) -- next to Apply button
favoriteButton.Text = "‚≠ê Favorite Colors"
favoriteButton.BackgroundColor3 = Color3.fromRGB(70,100,140)
favoriteButton.TextColor3 = Color3.new(1,1,1)
favoriteButton.Parent = mainFrame

-- Load/save
local HttpService = game:GetService("HttpService")
local SAVE_FILE = "FavoriteColors.json"
local FavData = { Categories = {}, NextCategoryId = 1 }

if isfile(SAVE_FILE) then
    local success, result = pcall(function()
        return HttpService:JSONDecode(readfile(SAVE_FILE))
    end)
    if success and result then
        FavData = result
    end
end

local function SaveFavData()
    writefile(SAVE_FILE, HttpService:JSONEncode(FavData))
end

-- Palette window variable
local paletteWindow

favoriteButton.MouseButton1Click:Connect(function()
    if paletteWindow then return end -- already open

    paletteWindow = Instance.new("Frame")
    paletteWindow.Size = UDim2.new(0,320,0,250)
    paletteWindow.Position = UDim2.new(0.5,-160,0.5,-125)
    paletteWindow.BackgroundColor3 = Color3.fromRGB(30,30,30)
    paletteWindow.BorderSizePixel = 0
    paletteWindow.Active = true
    paletteWindow.Draggable = true
    paletteWindow.Parent = screenGui -- must be your ScreenGui

    -- Close
    local closeBtn = Instance.new("TextButton", paletteWindow)
    closeBtn.Size = UDim2.new(0,50,0,25)
    closeBtn.Position = UDim2.new(1,-55,0,5)
    closeBtn.Text = "‚úñ"
    closeBtn.BackgroundColor3 = Color3.fromRGB(150,50,50)
    closeBtn.TextColor3 = Color3.new(1,1,1)
    closeBtn.MouseButton1Click:Connect(function()
        paletteWindow:Destroy()
        paletteWindow = nil
    end)

    -- Delete toggle
    local DeleteMode = false
    local DeleteButton = Instance.new("TextButton", paletteWindow)
    DeleteButton.Size = UDim2.new(0,60,0,25)
    DeleteButton.Position = UDim2.new(0,250,0,5)
    DeleteButton.Text = "Del: OFF"
    DeleteButton.BackgroundColor3 = Color3.fromRGB(120,70,70)
    DeleteButton.TextColor3 = Color3.new(1,1,1)
    DeleteButton.MouseButton1Click:Connect(function()
        DeleteMode = not DeleteMode
        DeleteButton.Text = "Del: "..(DeleteMode and "ON" or "OFF")
    end)

    -- Scroll
    local Scroll = Instance.new("ScrollingFrame", paletteWindow)
    Scroll.Size = UDim2.new(1,-10,1,-40)
    Scroll.Position = UDim2.new(0,5,0,35)
    Scroll.CanvasSize = UDim2.new(0,0,0,0)
    Scroll.ScrollBarThickness = 6
    Scroll.BackgroundColor3 = Color3.fromRGB(40,40,40)

    local Layout = Instance.new("UIListLayout", Scroll)
    Layout.Padding = UDim.new(0,5)
    Layout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
        Scroll.CanvasSize = UDim2.new(0,0,0,Layout.AbsoluteContentSize.Y + 5)
    end)

    -- Category creation
    local function CreateCategory(id)
        local CategoryFrame = Instance.new("Frame")
        CategoryFrame.Size = UDim2.new(1,-10,0,80)
        CategoryFrame.BackgroundColor3 = Color3.fromRGB(60,60,60)
        CategoryFrame.Parent = Scroll

        local Title = Instance.new("TextLabel", CategoryFrame)
        Title.Size = UDim2.new(0.6,0,0,20)
        Title.Text = "Category "..id
        Title.BackgroundTransparency = 1
        Title.TextColor3 = Color3.new(1,1,1)

        local AddButton = Instance.new("TextButton", CategoryFrame)
        AddButton.Size = UDim2.new(0,20,0,20)
        AddButton.Position = UDim2.new(1,-25,0,0)
        AddButton.Text = "+"
        AddButton.BackgroundColor3 = Color3.fromRGB(80,80,150)
        AddButton.TextColor3 = Color3.new(1,1,1)

        local ColorsFrame = Instance.new("Frame", CategoryFrame)
        ColorsFrame.Position = UDim2.new(0,5,0,25)
        ColorsFrame.Size = UDim2.new(1,-10,0,50)
        ColorsFrame.BackgroundTransparency = 1

        local Grid = Instance.new("UIGridLayout", ColorsFrame)
        Grid.CellSize = UDim2.new(0,20,0,20)
        Grid.CellPadding = UDim2.new(0,3,0,3)

        -- Load saved colors
        if FavData.Categories[id] then
            for _,rgb in ipairs(FavData.Categories[id]) do
                local btn = Instance.new("TextButton", ColorsFrame)
                btn.BackgroundColor3 = Color3.fromRGB(rgb[1],rgb[2],rgb[3])
                btn.Text = ""
                btn.MouseButton1Click:Connect(function()
                    if DeleteMode then
                        btn:Destroy()
                        for i,v in ipairs(FavData.Categories[id]) do
                            if v[1]==rgb[1] and v[2]==rgb[2] and v[3]==rgb[3] then
                                table.remove(FavData.Categories[id],i)
                                break
                            end
                        end
                        SaveFavData()
                    else
                        rBox.Text = rgb[1]
                        gBox.Text = rgb[2]
                        bBox.Text = rgb[3]
                        preview.BackgroundColor3 = Color3.fromRGB(rgb[1],rgb[2],rgb[3])
                    end
                end)
            end
        end

        -- Add new color
        AddButton.MouseButton1Click:Connect(function()
            local r = clamp(rBox.Text)
            local g = clamp(gBox.Text)
            local b = clamp(bBox.Text)
            FavData.Categories[id] = FavData.Categories[id] or {}
            table.insert(FavData.Categories[id],{r,g,b})
            SaveFavData()

            local btn = Instance.new("TextButton", ColorsFrame)
            btn.BackgroundColor3 = Color3.fromRGB(r,g,b)
            btn.Text = ""
            btn.MouseButton1Click:Connect(function()
                if DeleteMode then
                    btn:Destroy()
                    for i,v in ipairs(FavData.Categories[id]) do
                        if v[1]==r and v[2]==g and v[3]==b then
                            table.remove(FavData.Categories[id],i)
                            break
                        end
                    end
                    SaveFavData()
                else
                    rBox.Text = r
                    gBox.Text = g
                    bBox.Text = b
                    preview.BackgroundColor3 = Color3.fromRGB(r,g,b)
                end
            end)
        end)
    end

    -- Load saved categories
    for id,_ in pairs(FavData.Categories) do
        CreateCategory(id)
    end

    -- New category button
    local NewCategory = Instance.new("TextButton", paletteWindow)
    NewCategory.Size = UDim2.new(0,100,0,25)
    NewCategory.Position = UDim2.new(0,10,0,5)
    NewCategory.Text = "New Cat"
    NewCategory.BackgroundColor3 = Color3.fromRGB(70,100,140)
    NewCategory.TextColor3 = Color3.new(1,1,1)
    NewCategory.MouseButton1Click:Connect(function()
        local id = FavData.NextCategoryId
        FavData.Categories[id] = {}
        FavData.NextCategoryId += 1
        SaveFavData()
        CreateCategory(id)
    end)
end)

