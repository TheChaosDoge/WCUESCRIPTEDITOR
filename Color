local function applyPaletteColor(c)
	rBox.Text = math.floor(c.R * 255)
	gBox.Text = math.floor(c.G * 255)
	bBox.Text = math.floor(c.B * 255)
	preview.BackgroundColor3 = c
end

for _, color in ipairs(paletteColors) do
	local btn = Instance.new("TextButton")
	btn.Size = UDim2.new(0,30,0,30)
	btn.BackgroundColor3 = color
	btn.Text = ""
	btn.Parent = paletteScroll
	btn.MouseButton1Click:Connect(function()
		applyPaletteColor(color)
	end)
end
